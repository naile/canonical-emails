<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net46</TargetFrameworks>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageProjectUrl>https://github.com/naile/canonical-emails</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>
      CanonicalEmails provides normalization for email addresses. Supports gmail, hotmail, outlook, yahoo, protonmail and a few more
    </Description>
    <Authors>Andreas Caravella</Authors>
    <RepositoryUrl>https://github.com/naile/canonical-emails</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>1.0.2</Version>
  </PropertyGroup>

</Project>
